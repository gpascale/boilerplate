{
  "name": "boilerplate",
  "version": "1.0.0",
  "description": "React/Webpack boilerplate",
  "main": "server.js",
  "scripts": {
    "dev": "webpack-dev-server --config webpack.config.js --watch --colors",
    "build": "npm test && rm -rf dist/ && NODE_ENV=production webpack --colors --profile",
    "test": "mocha --compilers js:babel-core/register",
    "start": "PORT=8080 pm2 start ./server.js"
  },
  "author": "Greg Pascale (greg.pascale@gmail.com)",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.7",
    "babel-plugin-transform-object-rest-spread": "^6.19.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.25.0",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.24.1",
    "mocha": "^3.2.0",
    "node-sass": "^3.11.2",
    "pm2": "^2.1.4",
    "react-dom": "^15.3.2",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "underscore": "^1.8.3",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2"
  },
  "dependencies": {
    "react": "^15.3.2",
    "react-router": "^3.0.0"
  }
}
